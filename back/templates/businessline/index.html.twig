{% extends 'base.html.twig' %}

{% block title %}Businessline index{% endblock %}

{% block body %}
    <h1>Businessline index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for businessline in businesslines %}
            <tr>
                <td>{{ businessline.id }}</td>
                <td>{{ businessline.name }}</td>
                <td>
                    <a href="{{ path('businessline_show', {'id': businessline.id}) }}">show</a>
                    <a href="{{ path('businessline_edit', {'id': businessline.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('businessline_new') }}">Create new</a>
{% endblock %}
